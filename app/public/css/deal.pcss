    clear: both;
  }
  .row-item:hover {
    background-color: #6ACAFF;
  }
  .th,
  .td {
    float: left;
    text-align: center;
    color: white;
  }
  .th {
    height: 28px;
    line-height: 28px;
    vertical-align: bottom;
    color: white;
  }
  .td {
    font-size: 14px;
    height: 30px;
    line-height: 30px;
    cursor: pointer;
  }
  .col-1 {
    width: 33.33%;
  }
  .col-2 {
    width: 33.33%;
  }
  .col-3 {
    width: 33.33%;
  }
}


