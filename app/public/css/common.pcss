.modal-confirm-davao {
  .ant-modal-close {
    display: none;
  }
  .btn-cancel,
  .btn-submit {
    position: relative;
    float: left;
    width: 50%;
    border: none;
    color: #0DA9FF !important;
    background-color: transparent !important;
    margin: 0;
    font-size: 16px;
    box-shadow: none;
  }
  .btn-cancel:after {
    content: '';
    position: absolute;
    top: -3px;
    bottom: -3px;
    right: 0;
    width: 1px;
    background-color: #e8e8e8;
  }
  .ant-modal-footer {
    padding: 3px 16px;
    &:after {
      content: '';
      display: block;
      clear: both;
    }
  }
  .davao-confirm-wrap {
    .title {
      font-size: 16px;
      color: #111111;
      text-align: center;
      margin-bottom: 25px;
      &.simple {
        margin-bottom: 0;
      }
    }
    .password-value input {
      width: 35px;
      height: 35px;
      border: solid 1px #b5b5b5;
      margin-right: 11px;
      font-size: 16px;
      text-align: center;
      color: #111111;
    }
    .error {
      margin-top: 10px;
      font-size: 12px;
      color: #ff3f3f;
    }
  }
}

