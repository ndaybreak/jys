.hide {
  display: none !important;
}

.tai-slick-next {
  position: absolute;
  top: 34%;
  right: 15px;
  cursor: pointer;
}

.tai-slick-prev {
  position: absolute;
  top: 34%;
  left: 15px;
  cursor: pointer;
