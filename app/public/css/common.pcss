  .ant-modal-body {
    padding: 0;
  }
  .ant-modal-confirm-title {
    padding: 44px 32px 7px;
    text-align: center;
    color: #111111;
    font-size: 20px;
  }
  .ant-modal-confirm-btns {
    display: block;
    width: 100%;
    & > button:first-child,
    & > button:last-child {
      float: left;
      width: 50%;
      margin: 0;
      padding: 0;
      height: 50px;
      font-size: 16px;
    }
    & > button:first-child {
      color: #0DA9FF;
      border-top-left-radius: 0;
      border-top-right-radius: 0;
      border-bottom-right-radius: 0;
    }
    & > button:last-child {
      border-top-right-radius: 0;
      border-top-left-radius: 0;
      border-bottom-left-radius: 0;
    }
  }
}

.sto-modal-simple-confirm {
