#app {
  .app-inner {
    position: relative;
    width: 100%;
    height: 100%;
  }

  position: relative;
  width: 100%;
  height: 100%;

  .a-header {
    position: relative;
    height: 55px;
    padding-top: 10px;
    line-height: 40px;
    color: #ffffff;
    background-color: #15181f;
    z-index: 50;

    .header-wrap {
      width: 1200px;
      height: 45px;
      margin: auto;
      background-color: #15181f;

      .logo-wrap {
        position: absolute;
        left: 0;
        width: 95px;
        text-align: right;
        background: url("../img/logo.png") no-repeat center;
        height: 40px;
        cursor: pointer;
      }

      .content {
        position: relative;
        float: left;
        width: 1200px;
        margin: auto;
        background-color: #15181f;
        padding-left: 95px;

        .nav {
          float: left;
          padding-left: 10px;

          .menu {
            height: 45px;
            position: relative;
            float: left;
            width: 103px;
            text-align: center;
            color: #ffffff;

            .submenu-wrap {
              position: absolute;
              background-color: #0da9ff;
              left: 0;
              top: 45px;
              min-width: 103px;
              text-align: center;
              z-index: 5;

              li {
                height: 35px;
                line-height: 35px;

                .sub-menu {
                  color: #ffffff;
                  display: block;
                  height: 35px;
                  text-align: left;
                  text-align: center;
                }

                .sub-menu:hover {
                  background-color: #6ACAFF;
                }
              }
            }
          }

          .active {
            color: #0DA9FF;
            border-bottom: 1px solid #0DA9FF;
          }

          .menu:hover {
            background-color: #0da9ff;
            color: white;
          }
        }

        .login-register {
          float: right;

          .btn {
            display: inline-block;
            color: white;
          }
        }
      }
    }
  }

  .a-footer {
    position: relative;
    box-sizing: border-box;
    background-color: #15181f;
    margin-top: -167px;
    min-height: 167px;
    font-size: 13px;
    a {
      color: #888F9E;
      &:hover {
        text-decoration: underline;
      }
    }
    .footer-wrap {
      width: 1200px;
      padding-top: 14px;
      padding-bottom: 10px;
      margin: 0 auto;
      flex-direction: row;
      flex-wrap: nowrap;
      display: flex;
      align-items: flex-start;
      justify-content: space-between;
    }

    .link-block-wrap {
      padding-right: 50px;
      padding-top: 6px;

      .link-block-title {
        margin-left: 12px;
        font-family:Microsoft YaHei;
        font-size: 16px;
        line-height: 28px;
        color: rgba(255, 255, 255, 1);
      }

      .company-address-wrap {
        margin-top: 7px;
        margin-left: 12px;
      }

      .company-address {
        line-height: 32px;
        font-family:Microsoft YaHei;
        color: #888F9E;

        span {
          font-family:Microsoft YaHei;
          color: rgba(255, 255, 255, 1);
        }
      }

      .link-item-wrap {
        color: rgba(255, 255, 255, 1);
        margin-top: 12px;
        margin-bottom: 1px;
        list-style-position: inside;

        .link-item-img {
          width: 5px;
          height: 5px;
          margin-right: 7px;
        }

        .link-block-item {
          font-family:Microsoft YaHei;
          height: 12px;
          line-height: 21px;
          text-decoration: none;
          color: #888F9E;
        }
      }
    }

    .logo {
      padding-right: 50px;

      .logo-font {
        font-size: 24px;
        color: rgba(255, 255, 255, 1);
      }

      .support-wrap {
        margin-top: 14px;
        margin-bottom: 9px;
      }

      .support-email {
        font-family:Microsoft YaHei;
        margin-left: 7px;
        line-height: 21px;
        color: #888F9E;
        align-self: center;
        text-align: center;

        span {
          font-family:Microsoft YaHei;
          color: rgba(255, 255, 255, 1);
        }
      }

      .support-email-img {
        height: 17px;
        border: 0;
        filter: grayscale(100%)  brightness(200%);
        -webkit-filter: grayscale(100%) brightness(200%);
        -moz-filter: grayscale(100%) brightness(200%);
        -o-filter: grayscale(100%) brightness(200%);
      }

      .out-link {
        display: flex;
        flex-direction: row;
        flex-wrap: nowrap;
        align-items: center;
        margin-top: 14px;
        justify-content: space-between;

        .out-link-wrap {
          flex: 1;

          .out-link-img {
            height: 20px;
            filter: grayscale(100%)  brightness(200%);
            -webkit-filter: grayscale(100%) brightness(200%);
            -moz-filter: grayscale(100%) brightness(200%);
            -o-filter: grayscale(100%) brightness(200%);
            border: 0;
          }

          .out-link-img:hover {
            filter: none
          }
        }
      }
    }
  }

  .a-content {
    width: 100%;
    min-height: 100%;
    margin-top: -55px;
    padding-top: 55px;
    padding-bottom: 167px;
    position: relative;
  }
}

