}

.icon {
  display: inline-block;
}
.icon-arrow-down {
  width: 0;
  height: 0;
  border-top: 10px solid white;
  border-left: 7px solid transparent;
  border-right: 7px solid transparent;
  margin-top: 0;
  position: relative;
  top: 2px;
  margin-left: 3px;
}

.box-wrap {
  float: left;
  min-width: 100px;
  .box {
    width: 100%;
    height: 31px;
    background-color: #ffffff;
    border: solid 1px #b5b5b5;
    color: #111111;
    font-size: 14px;
    padding: 0 10px;
    border-radius: 0;
    &:focus {
      border-color: #0DA9FF;
    }
  }
  .box-time {
    float: left;
    width: 100%;
    .ant-input {
      width: 100%;
      height: 31px;
      background-color: #ffffff;
      border: solid 1px #b5b5b5;
      color: #111111;
      font-size: 14px;
      padding: 0 10px;
      float: left;
      border-radius: 0;
      &:focus {
        border-color: #0DA9FF;
      }
    }
  }
  .box-select {
    float: left;
    width: 100%;
    .ant-select-selection {
      width: 100%;
      height: 31px;
      background-color: #ffffff;
      border: solid 1px #b5b5b5; color: #111111;
      font-size: 14px;
      padding: 0 10px;
      float: left;
      border-radius: 0;
      &:hover {
        border-color: #0DA9FF;
      }
    }
    .ant-select-selection__rendered {
      margin-left: 0;
    }
  }

  .box-number {
    position: relative;
    display: inline-block;
    .box-number-unit {
      position: absolute;
      color: #b5b5b5;
      font-size: 12px;
      top: 5px;
      right: 17px;
      -moz-user-select:none;
      -webkit-user-select:none;
      user-select:none;
    }
    .box-number-input {
      height: 31px;
      line-height: 31px;
      width: 100%;
      border: 1px solid #b5b5b5;
      padding-left: 7px;
      &:focus {
        border-color: #0DA9FF;
      }
    }
  }

  .box-invalid {
    border-color: #ff3f3f !important;
    .ant-input {
      border-color: #ff3f3f !important;
    }
  }
  .box-error {
    height: 25px;
    font-size: 14px;
    color: #ff3f3f;
  }
}
.box-time-wrap .box-error,
.box-select-wrap .box-error {
  overflow: hidden;
  width: 100%;
}

.box-invalid .box-select .ant-select-selection {
  border-color: #ff3f3f;
}

.tip-success {
